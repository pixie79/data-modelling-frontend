<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                  id="Definitions_MartBuild"
                  targetNamespace="http://jaffleshop.example/bpmn">

  <bpmn:process id="js-process-mart-build" name="Mart Build" isExecutable="true">
    <bpmn:documentation>
      Gold Layer ETL Process - Builds dimensional model (star schema) for analytics.
      Creates dimension and fact tables optimized for BI queries and MetricFlow.
      Triggered after Staging Transformation completes.
    </bpmn:documentation>

    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_1" name="Staging Complete">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>

    <!-- Build Time Spine First (no dependencies) -->
    <bpmn:serviceTask id="Task_BuildTimeSpine" name="Build Time Spine">
      <bpmn:documentation>
        Generate metricflow_time_spine dimension:
        - Create date range from min(ordered_at) to current_date + 365
        - Calculate all date parts (week, month, quarter, year)
        - Calculate day-of metrics for filtering
      </bpmn:documentation>
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:serviceTask>

    <!-- Build Dimensions (can be parallel) -->
    <bpmn:parallelGateway id="Gateway_DimSplit" name="Build Dimensions in Parallel">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
    </bpmn:parallelGateway>

    <bpmn:serviceTask id="Task_BuildCustomers" name="Build Customers Dimension">
      <bpmn:documentation>
        Build customers dimension with lifetime metrics:
        - Calculate first_order_date, most_recent_order_date
        - Aggregate lifetime_order_count, lifetime_order_value
        - Join customer data with order aggregates
      </bpmn:documentation>
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Task_BuildProducts" name="Build Products Dimension">
      <bpmn:documentation>
        Build products dimension:
        - Copy from stg_products
        - Add any derived attributes
      </bpmn:documentation>
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_8</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Task_BuildLocations" name="Build Locations Dimension">
      <bpmn:documentation>
        Build locations dimension:
        - Copy from stg_locations
        - Add any derived attributes
      </bpmn:documentation>
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_9</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Task_BuildSupplies" name="Build Supplies Dimension">
      <bpmn:documentation>
        Build supplies dimension:
        - Copy from stg_supplies
        - Ensure product_id references are valid
      </bpmn:documentation>
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_10</bpmn:outgoing>
    </bpmn:serviceTask>

    <!-- Join dimension builds -->
    <bpmn:parallelGateway id="Gateway_DimJoin" name="Wait for Dimensions">
      <bpmn:incoming>Flow_7</bpmn:incoming>
      <bpmn:incoming>Flow_8</bpmn:incoming>
      <bpmn:incoming>Flow_9</bpmn:incoming>
      <bpmn:incoming>Flow_10</bpmn:incoming>
      <bpmn:outgoing>Flow_11</bpmn:outgoing>
    </bpmn:parallelGateway>

    <!-- Build Facts (depend on dimensions) -->
    <bpmn:serviceTask id="Task_BuildOrders" name="Build Orders Fact">
      <bpmn:documentation>
        Build orders fact table:
        - Extract order_date from ordered_at
        - Calculate item_count from order_items
        - Determine is_first_order flag per customer
        - Validate FK references to dimensions
      </bpmn:documentation>
      <bpmn:incoming>Flow_11</bpmn:incoming>
      <bpmn:outgoing>Flow_12</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Task_BuildOrderItems" name="Build Order Items Fact">
      <bpmn:documentation>
        Build order_items fact table:
        - Join with products for product_price
        - Calculate supply_cost from supplies
        - Copy is_food_item, is_drink_item flags
      </bpmn:documentation>
      <bpmn:incoming>Flow_12</bpmn:incoming>
      <bpmn:outgoing>Flow_13</bpmn:outgoing>
    </bpmn:serviceTask>

    <!-- Final Validation -->
    <bpmn:serviceTask id="Task_ValidateMart" name="Validate Mart Integrity">
      <bpmn:documentation>
        Run final validation checks:
        - Verify all FK relationships
        - Check for orphaned records
        - Validate aggregation accuracy
        - Run MetricFlow semantic validation
      </bpmn:documentation>
      <bpmn:incoming>Flow_13</bpmn:incoming>
      <bpmn:outgoing>Flow_14</bpmn:outgoing>
    </bpmn:serviceTask>

    <!-- End Event -->
    <bpmn:endEvent id="EndEvent_1" name="Mart Build Complete">
      <bpmn:incoming>Flow_14</bpmn:incoming>
    </bpmn:endEvent>

    <!-- Sequence Flows -->
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="Task_BuildTimeSpine"/>
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_BuildTimeSpine" targetRef="Gateway_DimSplit"/>
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Gateway_DimSplit" targetRef="Task_BuildCustomers"/>
    <bpmn:sequenceFlow id="Flow_4" sourceRef="Gateway_DimSplit" targetRef="Task_BuildProducts"/>
    <bpmn:sequenceFlow id="Flow_5" sourceRef="Gateway_DimSplit" targetRef="Task_BuildLocations"/>
    <bpmn:sequenceFlow id="Flow_6" sourceRef="Gateway_DimSplit" targetRef="Task_BuildSupplies"/>
    <bpmn:sequenceFlow id="Flow_7" sourceRef="Task_BuildCustomers" targetRef="Gateway_DimJoin"/>
    <bpmn:sequenceFlow id="Flow_8" sourceRef="Task_BuildProducts" targetRef="Gateway_DimJoin"/>
    <bpmn:sequenceFlow id="Flow_9" sourceRef="Task_BuildLocations" targetRef="Gateway_DimJoin"/>
    <bpmn:sequenceFlow id="Flow_10" sourceRef="Task_BuildSupplies" targetRef="Gateway_DimJoin"/>
    <bpmn:sequenceFlow id="Flow_11" sourceRef="Gateway_DimJoin" targetRef="Task_BuildOrders"/>
    <bpmn:sequenceFlow id="Flow_12" sourceRef="Task_BuildOrders" targetRef="Task_BuildOrderItems"/>
    <bpmn:sequenceFlow id="Flow_13" sourceRef="Task_BuildOrderItems" targetRef="Task_ValidateMart"/>
    <bpmn:sequenceFlow id="Flow_14" sourceRef="Task_ValidateMart" targetRef="EndEvent_1"/>
  </bpmn:process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="js-process-mart-build">
      <bpmndi:BPMNShape id="StartEvent_1_di" bpmnElement="StartEvent_1">
        <dc:Bounds x="152" y="192" width="36" height="36"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_BuildTimeSpine_di" bpmnElement="Task_BuildTimeSpine">
        <dc:Bounds x="240" y="170" width="100" height="80"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Gateway_DimSplit_di" bpmnElement="Gateway_DimSplit">
        <dc:Bounds x="395" y="185" width="50" height="50"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_BuildCustomers_di" bpmnElement="Task_BuildCustomers">
        <dc:Bounds x="500" y="60" width="100" height="80"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_BuildProducts_di" bpmnElement="Task_BuildProducts">
        <dc:Bounds x="500" y="150" width="100" height="80"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_BuildLocations_di" bpmnElement="Task_BuildLocations">
        <dc:Bounds x="500" y="240" width="100" height="80"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_BuildSupplies_di" bpmnElement="Task_BuildSupplies">
        <dc:Bounds x="500" y="330" width="100" height="80"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Gateway_DimJoin_di" bpmnElement="Gateway_DimJoin">
        <dc:Bounds x="655" y="185" width="50" height="50"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_BuildOrders_di" bpmnElement="Task_BuildOrders">
        <dc:Bounds x="760" y="170" width="100" height="80"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_BuildOrderItems_di" bpmnElement="Task_BuildOrderItems">
        <dc:Bounds x="920" y="170" width="100" height="80"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_ValidateMart_di" bpmnElement="Task_ValidateMart">
        <dc:Bounds x="1080" y="170" width="100" height="80"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="EndEvent_1_di" bpmnElement="EndEvent_1">
        <dc:Bounds x="1242" y="192" width="36" height="36"/>
      </bpmndi:BPMNShape>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>
