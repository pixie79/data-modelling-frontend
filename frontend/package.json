{
  "name": "open-data-modelling",
  "version": "2.4.2",
  "description": "Open Data Modelling - A domain-centric data modeling application",
  "main": "dist-electron/main.cjs",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build:wasm": "bash scripts/build-wasm.sh || echo 'WASM SDK build skipped (SDK not found or wasm-pack not installed)'",
    "build:duckdb": "bash scripts/copy-duckdb-wasm.sh",
    "build:all-wasm": "npm run build:wasm && npm run build:duckdb",
    "prebuild": "npm run build:all-wasm || true",
    "dev": "npm run build:duckdb && vite",
    "build": "tsc && vite build",
    "verify:versions": "node scripts/verify-versions.js",
    "build:electron": "vite build --config vite.electron.config.ts",
    "build:electron:frontend": "VITE_ELECTRON_BUILD=true vite build && npm run copy:wasm:electron",
    "copy:wasm:electron": "mkdir -p dist-electron/wasm dist-electron/duckdb && cp -r public/wasm/* dist-electron/wasm/ 2>/dev/null || cp -r dist/wasm/* dist-electron/wasm/ 2>/dev/null || true && cp -r public/duckdb/* dist-electron/duckdb/ 2>/dev/null || true",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage --reporter=json-summary --reporter=text",
    "test:e2e": "playwright test",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 150",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "type-check": "tsc --noEmit",
    "electron": "npm run build:electron && npm run build:electron:frontend && NODE_ENV=production electron .",
    "electron:dev": "npm run build:electron && concurrently \"npm run dev\" \"wait-on http://localhost:5173 && NODE_ENV=development electron .\"",
    "electron:build": "npm run build:electron:frontend && npm run build:electron && electron-builder",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "rebuild:examples": "node scripts/rebuild-examples.cjs",
    "rebuild:examples:migrate": "node scripts/rebuild-examples.cjs --migrate",
    "migrate:odcs": "node scripts/migrate-odcs.cjs",
    "prepare": "cd .. && git config core.hooksPath .husky || true",
    "pre-commit": "lint-staged"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@duckdb/duckdb-wasm": "^1.29.0",
    "@offenedatenmodellierung/data-modelling-sdk": "^2.0.6",
    "@tanstack/react-query": "^5.90.16",
    "@types/js-yaml": "^4.0.9",
    "@types/jszip": "^3.4.0",
    "axios": "^1.13.2",
    "bpmn-js": "^18.9.1",
    "dmn-js": "^17.5.0",
    "html-to-image": "^1.11.13",
    "js-yaml": "^4.1.1",
    "jszip": "^3.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.11.0",
    "reactflow": "^11.11.4",
    "remark-gfm": "^4.0.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/electron": "^1.6.12",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^15.2.11",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16",
    "wait-on": "^9.0.3"
  }
}
