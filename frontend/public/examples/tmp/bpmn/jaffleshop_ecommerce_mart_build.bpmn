      <bpmn:incoming>Flow_1x2oagu</bpmn:incoming>
      <bpmn:outgoing>Flow_11</bpmn:outgoing>
      <bpmn:outgoing>Flow_0as9dmd</bpmn:outgoing>
    </bpmn:parallelGateway>
    <bpmn:serviceTask id="Task_BuildOrders" name="Build Orders Fact">
      <bpmn:documentation>
        Build orders fact table:
        - Extract order_date from ordered_at
        - Calculate item_count from order_items
        - Determine is_first_order flag per customer
        - Validate FK references to dimensions
      </bpmn:documentation>
      <bpmn:incoming>Flow_11</bpmn:incoming>
      <bpmn:incoming>Flow_0as9dmd</bpmn:incoming>
      <bpmn:outgoing>Flow_12</bpmn:outgoing>
      <bpmn:outgoing>Flow_18b7e5p</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_BuildOrderItems" name="Build Order Items Fact">
      <bpmn:documentation>
        Build order_items fact table:
        - Join with products for product_price
        - Calculate supply_cost from supplies
        - Copy is_food_item, is_drink_item flags
      </bpmn:documentation>
      <bpmn:incoming>Flow_12</bpmn:incoming>
      <bpmn:incoming>Flow_18b7e5p</bpmn:incoming>
      <bpmn:outgoing>Flow_13</bpmn:outgoing>
      <bpmn:outgoing>Flow_05b9tdp</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_ValidateMart" name="Validate Mart Integrity">
      <bpmn:documentation>
        Run final validation checks:
        - Verify all FK relationships
        - Check for orphaned records
        - Validate aggregation accuracy
        - Run MetricFlow semantic validation
      </bpmn:documentation>
      <bpmn:incoming>Flow_13</bpmn:incoming>
      <bpmn:incoming>Flow_05b9tdp</bpmn:incoming>
      <bpmn:outgoing>Flow_14</bpmn:outgoing>
      <bpmn:outgoing>Flow_00fj47w</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:endEvent id="EndEvent_1" name="Mart Build Complete">
      <bpmn:incoming>Flow_14</bpmn:incoming>
      <bpmn:incoming>Flow_00fj47w</bpmn:incoming>
    </bpmn:endEvent>
