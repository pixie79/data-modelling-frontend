<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" id="Definitions_MartBuild" targetNamespace="http://jaffleshop.example/bpmn">
  <bpmn:process id="js-process-mart-build" name="Mart Build" isExecutable="true">
    <bpmn:documentation>
      Gold Layer ETL Process - Builds dimensional model (star schema) for analytics.
      Creates dimension and fact tables optimized for BI queries and MetricFlow.
      Triggered after Staging Transformation completes.
    </bpmn:documentation>
    <bpmn:startEvent id="StartEvent_1" name="Staging Complete">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
      <bpmn:outgoing>Flow_0vqlydv</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:serviceTask id="Task_BuildTimeSpine" name="Build Time Spine">
      <bpmn:documentation>
        Generate metricflow_time_spine dimension:
        - Create date range from min(ordered_at) to current_date + 365
        - Calculate all date parts (week, month, quarter, year)
        - Calculate day-of metrics for filtering
      </bpmn:documentation>
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:incoming>Flow_0vqlydv</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
      <bpmn:outgoing>Flow_1h1zw38</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:parallelGateway id="Gateway_DimSplit" name="Build Dimensions in Parallel">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:incoming>Flow_1h1zw38</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
      <bpmn:outgoing>Flow_03jiv0c</bpmn:outgoing>
      <bpmn:outgoing>Flow_1s5yj88</bpmn:outgoing>
      <bpmn:outgoing>Flow_08ohsjn</bpmn:outgoing>
      <bpmn:outgoing>Flow_1yvl2kl</bpmn:outgoing>
    </bpmn:parallelGateway>
    <bpmn:serviceTask id="Task_BuildCustomers" name="Build Customers Dimension">
      <bpmn:documentation>
        Build customers dimension with lifetime metrics:
        - Calculate first_order_date, most_recent_order_date
        - Aggregate lifetime_order_count, lifetime_order_value
        - Join customer data with order aggregates
      </bpmn:documentation>
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:incoming>Flow_1yvl2kl</bpmn:incoming>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
      <bpmn:outgoing>Flow_018s8px</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_BuildProducts" name="Build Products Dimension">
      <bpmn:documentation>
        Build products dimension:
        - Copy from stg_products
        - Add any derived attributes
      </bpmn:documentation>
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:incoming>Flow_08ohsjn</bpmn:incoming>
      <bpmn:outgoing>Flow_8</bpmn:outgoing>
      <bpmn:outgoing>Flow_1akcg6f</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_BuildLocations" name="Build Locations Dimension">
      <bpmn:documentation>
        Build locations dimension:
        - Copy from stg_locations
        - Add any derived attributes
      </bpmn:documentation>
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:incoming>Flow_1s5yj88</bpmn:incoming>
      <bpmn:outgoing>Flow_9</bpmn:outgoing>
      <bpmn:outgoing>Flow_0gy0wlt</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_BuildSupplies" name="Build Supplies Dimension">
      <bpmn:documentation>
        Build supplies dimension:
        - Copy from stg_supplies
        - Ensure product_id references are valid
      </bpmn:documentation>
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:incoming>Flow_03jiv0c</bpmn:incoming>
      <bpmn:outgoing>Flow_10</bpmn:outgoing>
      <bpmn:outgoing>Flow_1x2oagu</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:parallelGateway id="Gateway_DimJoin" name="Wait for Dimensions">
      <bpmn:incoming>Flow_7</bpmn:incoming>
      <bpmn:incoming>Flow_8</bpmn:incoming>
      <bpmn:incoming>Flow_9</bpmn:incoming>
      <bpmn:incoming>Flow_10</bpmn:incoming>
      <bpmn:incoming>Flow_018s8px</bpmn:incoming>
      <bpmn:incoming>Flow_1akcg6f</bpmn:incoming>
